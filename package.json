{
  "name": "ankiest",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "start:prod": "blitz start --port $PORT",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "migrate:list": "ts-eager -r dotenv/config -r tsconfig-paths/register -e 'require(\"./db/getMigrations\")'",
    "migrate:up": "ts-eager -r dotenv/config -r tsconfig-paths/register -e 'require(\"./db/migrateUp\")'",
    "migrate:down": "ts-eager -r dotenv/config -r tsconfig-paths/register -e 'require(\"./db/migrateDown\")'",
    "migrate:to": "ts-eager -r dotenv/config -r tsconfig-paths/register -e 'require(\"./db/migrateTo\")'",
    "migrate:latest": "ts-eager -r dotenv/config -r tsconfig-paths/register -e 'require(\"./db/migrateToLatest\")'"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/date-fns": "2.6.0",
    "@types/diff": "5.0.2",
    "@types/uuid": "8.3.4",
    "blitz": "0.45.2",
    "date-fns": "2.28.0",
    "diff": "5.0.0",
    "dotenv": "16.0.1",
    "final-form": "4.20.6",
    "kysely": "0.19.2",
    "papaparse": "5.3.2",
    "pg": "8.7.3",
    "react": "18.0.0-beta-149b420f6-20211119",
    "react-dom": "18.0.0-alpha-5ca4b0433-20211020",
    "react-final-form": "6.5.7",
    "ts-eager": "2.0.2",
    "tsconfig-paths": "4.0.0",
    "uuid": "8.3.2",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@types/papaparse": "5.3.2",
    "@types/pg": "8.6.5",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.38",
    "autoprefixer": "10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "tailwindcss": "3.0.18",
    "typescript": "~4.5"
  },
  "private": true
}
